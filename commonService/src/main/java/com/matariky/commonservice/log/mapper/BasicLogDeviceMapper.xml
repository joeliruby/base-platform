        insert into basic_log_device(id,system_version,access_terminals,access_module,business_time,access_url,access_interface,photoelectricity_time,device_id,device_collect_info,access_account,out_ip,login_address,log_time,remark,create_time,update_time,delete_time,create_by,update_by,operator_org_code,operator_self_org_code) 
