        </if>
        where id = #{params.id} and delete_time=0
	</update>

	<update id="delBasicBaseDeviceLogById" parameterType="int">
        update  basic_base_device_log set delete_time=now() where id=#{value}
	</update>

	<select id="getBasicBaseDeviceLogById"  parameterType="Integer" resultType="com.matariky.commonservice.device.bean.BasicBaseDeviceLog" >
