<sql id="baseQueryListSql">
 SELECT t.id,t.system_version_number,t.device_type,t.business_module,t.business_time,t.device_code,t.api_name,t.power,t.collect_content,t.physical_address,t.access_account,t.server_ip,t.create_time,t.update_time,t.delete_time,t.create_by,t.update_by,t.operator_org_code,t.operator_self_org_code,t.tenant_id
 FROM
    basic_base_device_log t
 WHERE
    t.delete_time = 0
 </sql>
 <sql id="baseQueryCountSql">
   SELECT
    count(1)
 FROM
    basic_base_device_log t
 WHERE
    t.delete_time = 0
 </sql>
	<select id="getBasicBaseDeviceLogAll" parameterType="com.matariky.commonservice.device.bean.BasicBaseDeviceLog" resultType="com.matariky.commonservice.device.bean.BasicBaseDeviceLog">
        select a.id,a.system_version_number,a.device_type,a.business_module,a.business_time,a.device_code,a.api_name,a.power,
               a.collect_content,a.physical_address,a.access_account,a.server_ip,a.create_time,a.update_time,a.delete_time,
               a.create_by,a.update_by,a.operator_org_code,a.operator_self_org_code,a.tenant_id , a.url , a.mac_address,a.trigger_time
        from basic_base_device_log a
        <where>
            a.delete_time=0
            <if test="params.accessAccount!=null and params.accessAccount!=''  ">
                and a.access_account  = #{params.accessAccount}
            </if>
	        <if test="params.businessTimeStart!=null and params.businessTimeStart!='' ">
	            <!--  Starting time search  -->
	            and a.business_time  &gt;= #{params.businessTimeStart}
	        </if>
	        <if test="params.businessTimeEnd!=null and params.businessTimeEnd!='' ">
	            <!--  End time search  -->
	            and a.business_time  &lt;= #{params.businessTimeEnd}
	        </if>
	        <if test="params.deviceType!=null and params.deviceType!=''  ">
	                and a.device_type  = #{params.deviceType}
	        </if>
	        <if test="params.deviceCode!=null and params.deviceCode!=''  ">
	                and a.device_code  = #{params.deviceCode}
	        </if>
            order by create_time  desc
        </where>
	</select>

	<select id="getBasicBaseDeviceLogAllCount" parameterType="Map" resultType="int">
        select count(1) from basic_base_device_log where delete_time=0
	</select>

	<insert id="createBasicBaseDeviceLog" parameterType="com.matariky.commonservice.device.bean.BasicBaseDeviceLog">
