<mapper namespace="com.matariky.commonservice.base.mapper.BasicBasePcVersionMapper">

<sql id="baseQueryListSql">
 SELECT t.id,t.version_name,t.version_no,t.version_content,t.requirement_date,t.message_shutdown_time,t.remark,t.create_time,t.update_time,t.delete_time,t.create_by,t.update_by,t.operator_org_code,t.operator_self_org_code
 FROM
    basic_base_pcversion t
 WHERE
    t.delete_time = 0
 </sql>
 <sql id="baseQueryCountSql">
   SELECT
    count(1)
 FROM
    basic_base_pcversion t
 WHERE
    t.delete_time = 0
 </sql>


	<select id="getBasicBasePcversionAll" parameterType="com.matariky.commonservice.base.vo.BasicBasePcVersionQueryVO" resultType="com.matariky.commonservice.base.vo.BasicBasePcVersionListVO">
        select p.id,p.version_name,p.version_no,p.version_content,p.requirement_date,p.message_shutdown_time,p.remark,p.create_time,
               p.update_time,p.delete_time,p.create_by,p.update_by,p.operator_org_code,p.operator_self_org_code , u.real_name
        from basic_base_pcversion p left join  user_user u on p.update_by = u.id
        <where>
             p.delete_time=0
             <if test="params.versionNo!=null and params.versionNo!='' ">
               and  p.version_no like concat('%',#{params.versionNo},'%')
             </if>
        </where>
	</select>

	<select id="getBasicBasePcversionAllCount" parameterType="Map" resultType="int">
        select count(1) from basic_base_pcversion where delete_time=0
	</select>

	<insert id="createBasicBasePcversion" parameterType="com.matariky.commonservice.base.bean.BasicBasePcVersion">
        insert into basic_base_pcversion(id,version_name,version_no,version_content,requirement_date,message_shutdown_time,remark,create_time,update_time,create_by,update_by,operator_org_code,operator_self_org_code,tenant_id)
        values(#{id},#{versionName},#{versionNo},#{versionContent},#{requirementDate},#{messageShutdownTime},#{remark},#{createTime},#{updateTime},#{createBy},#{updateBy},#{operatorOrgCode},#{operatorSelfOrgCode},#{tenantId})
	</insert>

	<update id="updateBasicBasePcversion" parameterType="com.matariky.commonservice.base.bean.BasicBasePcVersion">
        update basic_base_pcversion
        set version_name=#{versionName},version_no=#{versionNo},version_content=#{versionContent},requirement_date=#{requirementDate},message_shutdown_time=#{messageShutdownTime},remark=#{remark},create_time=#{createTime},update_time=#{updateTime},delete_time=#{deleteTime},create_by=#{createBy},update_by=#{updateBy},operator_org_code=#{operatorOrgCode},operator_self_org_code=#{operatorSelfOrgCode}
        where id = #{id} and delete_time=0
	</update>

	<update id="delBasicBasePcversionById" parameterType="java.lang.Long">
        update  basic_base_pcversion set delete_time=now() where id=#{value}
	</update>

	<select id="getBasicBasePcversionById"  parameterType="Integer" resultType="com.matariky.commonservice.base.bean.BasicBasePcVersion" >
        select id,version_name,version_no,version_content,requirement_date,message_shutdown_time,remark,create_time,update_time,delete_time,create_by,update_by,operator_org_code,operator_self_org_code from basic_base_pcversion where id=#{value} and delete_time=0
	</select>

