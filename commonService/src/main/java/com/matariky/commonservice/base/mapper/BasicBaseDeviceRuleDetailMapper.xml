        set rule_id=#{ruleId},filter_conditions=#{filterConditions},condition_setting=#{conditionSetting},set_value=#{setValue},remark=#{remark},create_time=#{createTime},update_time=#{updateTime},delete_time=#{deleteTime},create_by=#{createBy},update_by=#{updateBy},operator_org_code=#{operatorOrgCode},operator_self_org_code=#{operatorSelfOrgCode}
        where id = #{id} and delete_time=0
	</update>
    <update id="delBasicBaseDeviceruleDetailById" parameterType="java.lang.Long">
