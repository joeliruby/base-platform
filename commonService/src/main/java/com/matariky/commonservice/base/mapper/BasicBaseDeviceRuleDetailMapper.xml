        values(#{id},#{ruleId},#{filterConditions},#{conditionSetting},#{setValue},#{remark},#{createTime},#{updateTime},#{createBy},#{updateBy},#{operatorOrgCode},#{operatorSelfOrgCode})
	</insert>
