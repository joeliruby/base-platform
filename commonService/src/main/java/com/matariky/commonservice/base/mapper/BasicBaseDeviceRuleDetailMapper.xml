    <sql id="baseQueryListSql">
 SELECT t.id,t.rule_id,t.filter_conditions,t.condition_setting,t.set_value,t.remark,t.create_time,t.update_time,t.delete_time,t.create_by,t.update_by,t.operator_org_code,t.operator_self_org_code
 FROM
    basic_base_devicerule_detail t
 WHERE
    t.delete_time = 0
 </sql>
