 </sql>
    <select id="getBasicBaseDeviceruleDetailAll" parameterType="com.matariky.commonservice.base.vo.BasicBaseDeviceRuleDetailListVO" resultType="com.matariky.commonservice.base.vo.BasicBaseDeviceRuleDetailVO">
        select d.id,d.rule_id,d.filter_conditions,d.condition_setting,d.set_value,d.remark,d.create_time,
               d.update_time,d.delete_time,d.create_by,d.update_by,d.operator_org_code, d.operator_self_org_code ,
               u.real_name , c. rules_name setValueName
        from  basic_base_devicerule r
              left join   basic_base_devicerule_detail d  on r.id =d.rule_id
              left join   user_user u  on d.update_by = u.id
              left join  basic_base_codingrules c on c.id = d.set_value and c.status = 1
        where d.delete_time = 0 and  r.id =#{ruleId}
        order by  d.create_time asc
	</select>
