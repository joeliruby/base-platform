<sql id="baseQueryListSql">
 SELECT t.id,t.command_name,t.protocol_type,t.command_content,t.remark,t.create_time,t.update_time,t.delete_time,t.create_by,t.update_by,t.operator_org_code,t.operator_self_org_code,t.tenant_id,t.md5
 FROM
    basic_base_devicecommand t
 WHERE
    t.delete_time = 0
 </sql>
 <sql id="baseQueryCountSql">
   SELECT
    count(1)
 FROM
    basic_base_devicecommand t
 WHERE
    t.delete_time = 0
 </sql>
	<select id="getBasicBaseDevicecommandAll" parameterType="Map" resultType="com.matariky.commonservice.base.bean.BasicBaseDevicecommand">
        select id,command_name,protocol_type,command_content,remark,create_time,update_time,delete_time,
               create_by,update_by,operator_org_code,operator_self_org_code,tenant_id,md5 from basic_base_devicecommand t
                <where>
                    delete_time=0
                    <if test="params.protocolType!=null and params.protocolType!='' ">
                      and  protocol_type  =#{params.protocolType}
                    </if>
                </where>
	</select>

	<select id="getBasicBaseDevicecommandAllCount" parameterType="Map" resultType="int">
