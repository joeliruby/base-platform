        </if>
<if test="params.operatorSelfOrgCode != null ">
        	,operator_self_org_code=#{params.operatorSelfOrgCode}
        </if>
<if test="params.tenantId != null ">
        	,tenant_id=#{params.tenantId}
        </if>
<if test="params.md5 != null ">
        	,md5=#{params.md5}
        </if>
        where id = #{params.id} and delete_time=0
	</update>

	<update id="delBasicBaseDevicecommandById" parameterType="int">
        update  basic_base_devicecommand set delete_time=now() where id=#{value}
	</update>

	<select id="getBasicBaseDevicecommandById"  parameterType="Integer" resultType="com.matariky.commonservice.base.bean.BasicBaseDevicecommand" >
