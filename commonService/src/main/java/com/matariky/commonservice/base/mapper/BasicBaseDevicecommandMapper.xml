        update basic_base_devicecommand
        set id=#{params.id}



<if test="params.commandName != null ">
        	,command_name=#{params.commandName}
        </if>
<if test="params.protocolType != null ">
        	,protocol_type=#{params.protocolType}
        </if>
<if test="params.commandContent != null ">
