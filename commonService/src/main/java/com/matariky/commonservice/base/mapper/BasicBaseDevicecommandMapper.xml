	</select>

	<insert id="createBasicBaseDevicecommand" parameterType="com.matariky.commonservice.base.bean.BasicBaseDevicecommand">
        insert into basic_base_devicecommand(id,command_name,protocol_type,command_content,remark,create_time,update_time,create_by,update_by,operator_org_code,operator_self_org_code,tenant_id,md5)
        values(#{id},#{commandName},#{protocolType},#{commandContent},#{remark},#{createTime},#{updateTime},#{createBy},#{updateBy},#{operatorOrgCode},#{operatorSelfOrgCode},#{tenantId},#{md5})
	</insert>

