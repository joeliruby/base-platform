<sql id="baseQueryListSql"> 
 SELECT t.id,t.rfidfactory_id,t.goods_id,t.device_id,t.rfid_id,t.remark,t.create_time,t.update_time,t.delete_time,t.create_by,t.update_by,t.operator_org_code,t.operator_self_org_code,t.tenant_id 
 FROM
    basic_base_createrfid_factory t 
 WHERE 
    t.delete_time = 0 
 </sql> 
 <sql id="baseQueryCountSql"> 
