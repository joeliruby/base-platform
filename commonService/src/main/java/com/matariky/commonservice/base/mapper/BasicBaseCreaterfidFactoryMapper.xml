	</insert>

	<update id="updateBasicBaseCreaterfidFactory" parameterType="com.matariky.commonservice.base.bean.BasicBaseCreaterfidFactory">
        update basic_base_createrfid_factory 
        set rfidfactory_id=#{rfidfactoryId},goods_id=#{goodsId},device_id=#{deviceId},rfid_id=#{rfidId},remark=#{remark},create_time=#{createTime},update_time=#{updateTime},delete_time=#{deleteTime},create_by=#{createBy},update_by=#{updateBy},operator_org_code=#{operatorOrgCode},operator_self_org_code=#{operatorSelfOrgCode},tenant_id=#{tenantId}
        where id = #{id} and delete_time=0 
	</update>

	<update id="delBasicBaseCreaterfidFactoryById" parameterType="int">
        update  basic_base_createrfid_factory set delete_time=now() where id=#{value}
