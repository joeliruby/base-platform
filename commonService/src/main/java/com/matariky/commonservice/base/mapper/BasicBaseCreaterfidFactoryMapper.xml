<select id="getBasicBaseCreaterfidFactoryByFactoryId" parameterType="java.lang.Long" resultType="com.matariky.commonservice.base.bean.BasicBaseCreaterfidFactory">

    SELECT
        a.epc,
        a.password,
        b.goods_name,
        b.goods_code,
        a.od_content,
        a.qr_content
    FROM
        basic_base_createrfid_factory AS a left join
        basic_base_goods AS b on a.goods_id = b.id
    WHERE
        a.rfidfactory_id=#{value}

</select>

