	<select id="getBasicBaseRfidfactoryAll" resultType="com.matariky.bizservice.assetitm.base.bean.BasicBaseRfidfactory">
        select a.id,a.task_batch_code,a.task_name,a.goods_id,a.rfid_type,d.rules_name as epc_rule,a.rfid_num,a.yield_rate,a.create_num,a.is_lock_epc,a.epc_password,a.is_odcode,a.od_fixed_content,a.is_qrcode,a.qr_fixed_content,a.is_file_create,a.download_num,a.download_time,a.upload_num,a.task_status,a.remark,a.create_time,a.update_time,a.delete_time,a.create_by,a.update_by,a.operator_org_code,a.operator_self_org_code,b.goods_code,b.goods_name , u.real_name
        from basic_base_rfidfactory a left join basic_base_goods b on a.goods_id=b.id
        left join basic_base_codingrules d on a.epc_rule=d.id and d.status =1
        left join  user_user u on a.create_by = u.id

        <where>
            a.delete_time=0
            <if test="params.taskName!=null and params.taskName!=''  ">
                and a.task_name like concat('%',#{params.taskName},'%')
            </if>
            <if test="params.rfidType!=null and params.rfidType!='' ">
                and a.rfid_type =#{params.rfidType}
            </if>
            <if test="params.createDateStart!=null and params.createDateStart!='' ">
                <!--  开始时间检索  -->
                and DATE_FORMAT(FROM_UNIXTIME(a.create_time / 1000), '%Y-%m-%d %H:%i:%s') &gt;= #{params.createDateStart}
            </if>
            <if test="params.createDateEnd!=null and params.createDateEnd!='' ">
                <!--  结束时间检索  -->
                and DATE_FORMAT(FROM_UNIXTIME(a.create_time / 1000), '%Y-%m-%d %H:%i:%s')  &lt;= #{params.createDateEnd}
            </if>
        </where>
        order by  a.create_time desc
	</select>

