<sql id="baseQueryListSql">
 SELECT t.id,t.task_batch_code,t.task_name,t.goods_id,t.rfid_type,t.epc_rule,t.rfid_num,t.yield_rate,t.create_num,t.is_lock_epc,t.epc_password,t.is_odcode,t.od_fixed_content,t.is_qrcode,t.qr_fixed_content,t.is_file_create,t.download_num,t.download_time,t.task_status,t.remark,t.create_time,t.update_time,t.delete_time,t.create_by,t.update_by,t.operator_org_code,t.operator_self_org_code
 FROM
    basic_base_rfidfactory t
 WHERE
    t.delete_time = 0
 </sql>
 <sql id="baseQueryCountSql">
   SELECT
    count(1)
 FROM
