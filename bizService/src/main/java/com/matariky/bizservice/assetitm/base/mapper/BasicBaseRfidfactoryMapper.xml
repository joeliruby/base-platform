	<update id="updateBasicBaseRfidfactory" parameterType="com.matariky.bizservice.assetitm.base.bean.BasicBaseRfidfactory">
        update basic_base_rfidfactory
        set task_batch_code=#{taskBatchCode},task_name=#{taskName},goods_id=#{goodsId},rfid_type=#{rfidType},epc_rule=#{epcRule},rfid_num=#{rfidNum},yield_rate=#{yieldRate},create_num=#{createNum},is_lock_epc=#{isLockEpc},epc_password=#{epcPassword},is_odcode=#{isOdcode},od_fixed_content=#{odFixedContent},is_qrcode=#{isQrcode},qr_fixed_content=#{qrFixedContent},is_file_create=#{isFileCreate},download_num=#{downloadNum},download_time=#{downloadTime},task_status=#{taskStatus},remark=#{remark},create_time=#{createTime},update_time=#{updateTime},delete_time=#{deleteTime},create_by=#{createBy},update_by=#{updateBy},operator_org_code=#{operatorOrgCode},operator_self_org_code=#{operatorSelfOrgCode},download_url=#{downloadUrl}
        where id = #{id} and delete_time=0
	</update>

	<update id="delBasicBaseRfidfactoryById" parameterType="java.lang.Long">
        update  basic_base_rfidfactory set delete_time=now() where id=#{value}
	</update>

	<select id="getBasicBaseRfidfactoryById"  parameterType="java.lang.Long" resultType="com.matariky.bizservice.assetitm.base.bean.BasicBaseRfidfactory" >
